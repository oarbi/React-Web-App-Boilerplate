{
  "name": "react-web-app-boilerplate",
  "version": "0.0.1",
  "description": "A React Web App Example",
  "author": "Oussama ARBI <arbi.oussama.tn@gmail.com>",
  "license": "MIT",
  "main": "src/main.jsx",
  "scripts": {
    "start": "npx webpack-dev-server --config ./webpack.config.js --mode development",
    "build": "rm -rf dist/ && npx webpack",
    "build:prod": "rm -rf dist/ && NODE_ENV=production npx webpack",
    "lint": "npx eslint src --config ./eslint.config.js --ext .jsx",
    "lint:fix": "npx eslint src --config ./eslint.config.js --ext .jsx --fix",
    "test": "npx jest --config ./jest.config.js",
    "test:update": "npx jest --config ./jest.config.js -u",
    "list": "npx browserslist"
  },
  "dependencies": {
    "i18next": "^19.8.3",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.0.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.7.3",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.1",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.1",
